<%= form_for([@publisher, @comic]) do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title %>
<br>
  <%= f.label :issue_no %>
  <%= f.text_field :issue_no %>
<br>
  <%= f.label :authors %>
  <%= f.text_field :authors %>
<br>
  <%= f.label :artists %>
  <%= f.text_field :artists %>
<br>
  <%= f.label :year %>
  <%= f.text_field :year %>
<br>
  <%= f.label :cover_url %>
  <%= f.text_field :cover_url %>
<br>
  <%= f.submit %>

<% end %>
